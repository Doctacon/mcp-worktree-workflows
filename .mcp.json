{
    "mcpServers": {
      "worktree-voting": {
        "type": "stdio",
        "command": "uv",
        "args": [
            "run",
            "--no-project",
            "--with",
            "fastmcp",
            "./mcp-servers/worktree_workflows.py"
        ],
        "env": {}
      },
      "ElevenLabs": {
          "type": "stdio",
          "command": "uvx",
          "args": [
            "elevenlabs-mcp"
          ],
          "env": {
            "ELEVENLABS_API_KEY": "<sample-api-key>"
          }
        }
    }
}